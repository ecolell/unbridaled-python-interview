{
    "name": "Start on Heroku: Python",
    "description": "A barebones Python app, which can easily be deployed to Heroku.",
    "image": "heroku/python",
    "repository": "https://github.com/ecolell/unbridaled-python-interview",
    "keywords": ["python", "fastapi" ],
    "addons": [ "heroku-postgresql" ],
    "env": {
      "DATABASE_URL": {
        "description": "The connection url for the FastAPI application database."
      }
    },
    "buildpacks": [
        {
          "url": "https://github.com/heroku/heroku-buildpack-python.git"
        }
    ],
    "environments": {
      "test": {
        "scripts": {
          "test": "make pipeline-backend-test"
        }
      }
    }
  }
